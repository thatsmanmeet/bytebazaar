{
  "name": "bytebazaar",
  "version": "1.0.0",
  "description": "A ecommerce website for buying tech stuff",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "server": "node backend/server.js",
    "client": "npm run dev --prefix frontend",
    "dev": "concurrently \"nodemon backend/server.js\" \"npm run dev --prefix frontend \"",
    "build": "npm install && cd frontend && npm run build"
  },
  "keywords": [
    "ecommerce",
    "tech"
  ],
  "author": "thatsmanmeet",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.13.2",
    "multer": "^1.4.5-lts.2",
    "qrcode": "^1.5.4",
    "speakeasy": "^2.0.0",
    "validator": "^13.15.0"
  },
  "devDependencies": {
    "colors": "^1.4.0",
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.9"
  }
}
